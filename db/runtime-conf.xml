<?xml version="1.0" encoding="UTF-8"?>
<config>
  <propel>
    <datasources default="datawrapper">
      <datasource id="datawrapper">
        <adapter>mysql</adapter> <!-- sqlite, mysql, mssql, oracle, or pgsql -->
        <connection>
          <dsn>mysql:host=localhost;dbname=datawrapper</dsn>
          <!--
          This is the buildfile for Propel. Do NOT edit this file to configure your
          database connection, but rather copy conf/datawrapper-conf.php.master to
          conf/datawrapper-conf.php and change the credentials there.
          -->
          <user>root</user>
          <password></password>
        </connection>
      </datasource>
    </datasources>
  </propel>
</config>
